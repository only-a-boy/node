<!DOCTYPE html>
<html>
<head>
	<link rel="stylesheet" href="/stylesheets/basic.css">
	<link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
	<script src="/javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.min.js"></script>
	<title><%= title %></title>
</head>
</head>
<body>
<% include ../header %>

<form class="form-horizontal" action="/updateCar" method="post">
  <div class="form-group">
    <label class="col-sm-4 control-label">车辆序号:</label>
    <div class="col-sm-2">
      <p class="form-control-static" id="carId"></p>
    </div>
  </div>
    

  <div class="form-group">
    <label class="col-sm-4 control-label">VIN：</label>
    <div class="col-sm-2">
      <p class="form-control-static" id="vin"></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">发动机号：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="engineNo">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">车型：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="model">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">颜色：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="color">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">地址：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="address">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">车牌号：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="carCard">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">城市名：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="cityName">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">新产品年度：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="modelYear">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">购车日期：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="purchaseDate">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">车辆类型：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="carType">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">备注：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="memo">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">状态：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="status">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">创建人：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="createUserId">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 control-label">创建时间：</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="createTime">
    </div>
    <button class="btn btn-default" type="button" id="confirm">确认修改</button>
  </div>
</form>




<% include ../footer %>

<script src="bundle.js"></script>

</body>
</html>
