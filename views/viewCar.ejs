<!DOCTYPE html>
<html>
<head>
	<link rel="stylesheet" href="/stylesheets/basic.css">
	<link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
  <link rel="stylesheet" href="/stylesheets/viewCar.css">
	<script src="javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.min.js"></script>
	<title><%= title %></title>
</head>
</head>
<body>
<% include header %>

<form class="form-horizontal">
  <div class="form-group">
    <label class="col-sm-5 control-label">车辆序号:</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].car_id %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">VIN：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].vin %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">发动机号：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].engine_no %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">车型：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].model %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">颜色：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].color %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">地址：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].address %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">车牌号：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].car_card %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">城市名：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].city_name %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">新产品年度：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].model_year %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">购车日期：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].purchase_date %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">车辆类型：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].car_type %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">备注：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].memo %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">状态：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].status %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">创建人：</label>
    <div class="col-sm-7">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].create_user_id %></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-5 control-label">创建时间：</label>
    <div class="col-sm-2">
      <p class="form-control-static"><%= JSON.parse(carInfo)[0].create_time %></p>
    </div>
   
  </div>

</form>

 <button class="btn btn-default col-sm-offset-5"><a href="/car">返回</a></button>




<% include footer %>
</body>
</html>
